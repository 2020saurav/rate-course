extends adminLayout
block content
div
h2 Create a new Rating Value
form.form-horizontal(method='post', action ='')
    div.form-group
        label.col-md-2.control-label Rating ID
        div.col-md-10
            input.form-control(type="text" name="ratingId" placeholder="Rating ID")
    div.form-group
        label.col-md-2.control-label Rating Parameter ID
        div.col-md-10
            input.form-control(type="text" name="ratingParamId" placeholder="Rating Parameter ID")
    div.form-group
        label.col-md-2.control-label Value
        div.col-md-10
            input.form-control(type="text" name="value" placeholder = "Value")
    div.form-group
        button.btn.btn-primary.col-md-offset-10(type = 'submit') Submit
